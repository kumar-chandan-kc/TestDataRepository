<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form">
	<Dialog class="sapUiNoMargin sapUiNoContentPadding" id="SSCUIConvertToActualsSelectDialog" title="Deployment Options" resizable="true" draggable="true">
		<f:SimpleForm editable="true" layout="ResponsiveGridLayout">
			<f:content>
				<Label text="Release" design="Bold"/>
				<Input id="ReleaseDeploy" editable="true" showValueHelp="true" value="" showSuggestion="true" valueHelpOnly="true"
					valueHelpRequest="onValueHelpRequest">
				</Input>
				<Label text="System" design="Bold"/>
				<Input id="SystemDeploy" editable="true" showValueHelp="true" value="" showSuggestion="true" valueHelpOnly="true"
					valueHelpRequest="onSystemValueHelpRequest">
				</Input>
				<Label text="Client" design="Bold"/>
				<Input id="Client12" editable="true" showValueHelp="true" value="" showSuggestion="true" valueHelpOnly="true"
					valueHelpRequest="onClientValueHelp">
				</Input>
			</f:content>
		</f:SimpleForm>
		<buttons>
			<Button text="OK" press="fnOk"></Button>
			<Button text="Cancel" press="fnCancel"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>